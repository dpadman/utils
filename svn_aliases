alias si='svn info;'
alias upg='svn upgrade;'
alias upd='svn up --ignore-externals; ~/.cs.script; echo -n -e "\033kcsc\033\\"; cscope -d; echo -n -e "\033k$SHORTNAME\033\\";'
alias sup='svn up;'
alias sl='svn log | less'
alias sr='svn revert'
alias sd='svn diff | grep "Index:" | less'
alias sdf='svn diff'
alias sdc='svn diff --changelist'
alias scl='svn changelist'
alias sstat='svn status'
